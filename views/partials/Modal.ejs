<div class="modal fade" tabindex="-1" role="dialog" id="logout_modal" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered" role="content">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title text-center" id="exampleModalCenterTitle">Logout</h3>
                <button type="button" class="close" data-dismiss="modal" >&times;</button>
            </div>
            <div class="modal-body">
               <div class="row m-auto">
                <h3>Are you sure you want to logout?</h3>
               </div>
               <div class="row m-auto">
                <button type="button" class="btn btn-secondary btn-sm ml-auto " data-dismiss="modal">Cancel</button>
                <button class="btn btn-warning btn-sm ml-1"><a href="/logout">Yes</a></button>
               </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" tabindex="-1" role="dialog" id="del_modal" aria-labelledby="ModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered" role="content">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title text-center" id="exampleModalCenterTitle">Delete</h3>
                <button type="button" class="close" data-dismiss="modal" >&times;</button>
            </div>
            <div class="modal-body">
               <div class="row m-auto">
                <h3>Are you sure you want to delete?</h3>
               </div>
               <div class="row m-auto">
                <button type="button" class="btn btn-secondary btn-sm ml-auto " data-dismiss="modal">Cancel</button>
                
                <button class="btn btn-warning btn-sm ml-1 " id="delete-button" name=<%=page %> 
                    <% if(page!="notes") { %>
                    onclick="deleteTask(this.id,this.name)"
                    <% } else {%>
                    onclick="deleteNote(this.id,this.name)"
                    <% } %>
                    ><a>Yes</a></button>
               </div>
            </div>
        </div>
    </div>
</div>